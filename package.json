{
  "name": "hmpps-prisoner-profile",
  "version": "0.0.1",
  "description": "HMPPS Prisoner Profile",
  "repository": "git@github.com:ministryofjustice/hmpps-prisoner-profile.git",
  "license": "MIT",
  "scripts": {
    "prepare": "hmpps-precommit-hooks-prepare",
    "watch-ts": "tsc -w",
    "build": "node esbuild/esbuild",
    "start": "node $NODE_OPTIONS dist/server.js | bunyan -o short",
    "start:dev": "node esbuild/esbuild --watch",
    "start-feature": "node $NODE_DEBUG_OPTION --env-file=feature.env dist/server.js | bunyan -o short",
    "start-feature:dev": "node esbuild/esbuild --watch --env feature.env",
    "lint": "eslint . --cache --max-warnings 0",
    "lint-fix": "eslint . --cache --max-warnings 0 --fix",
    "typecheck": "tsc && tsc -p integration_tests",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --runInBand",
    "security_audit": "npx audit-ci --config audit-ci.json",
    "int-test": "cypress run",
    "int-test-ui": "cypress open --e2e --browser chrome",
    "integration-test-1": "cypress run --spec \"**/personal/edit/**/*.cy.ts\"",
    "integration-test-2": "cypress run --spec \"**/personal/editDistinguishingMarks/**/*.cy.ts\"",
    "integration-test-3": "cypress run --spec \"**/e2e/*.cy.ts,**/alerts/**/*.cy.ts,**/personal/*.cy.ts,**/photo/**/*,**/workAndSkillsPage/**/*.cy.ts\"",
    "clean": "rm -rf dist test_results",
    "rebuild": "npm run clean && npm run setup && npm run build",
    "setup": "npm ci && hmpps-npm-script-run-allowlist"
  },
  "engines": {
    "node": "^24",
    "npm": "^11"
  },
  "dependencies": {
    "@apollo/client": "4.1.4",
    "@aws-sdk/client-sqs": "3.985.0",
    "@contentful/rich-text-html-renderer": "^17.1.6",
    "@contentful/rich-text-types": "^17.2.5",
    "@microsoft/applicationinsights-clickanalytics-js": "3.3.11",
    "@microsoft/applicationinsights-web": "3.3.11",
    "@ministryofjustice/frontend": "^3.7.2",
    "@ministryofjustice/hmpps-auth-clients": "1.0.2",
    "@ministryofjustice/hmpps-connect-dps-components": "5.3.0",
    "@ministryofjustice/hmpps-connect-dps-shared-items": "3.5.2",
    "@ministryofjustice/hmpps-monitoring": "1.0.2",
    "@ministryofjustice/hmpps-npm-script-allowlist": "0.0.3",
    "@ministryofjustice/hmpps-prison-permissions-lib": "2.1.0",
    "@ministryofjustice/hmpps-rest-client": "1.0.1",
    "@sentry/node": "10.38.0",
    "@types/multer": "2.0.0",
    "accessible-autocomplete": "^3.0.1",
    "agentkeepalive": "^4.6.0",
    "applicationinsights": "^2.9.8",
    "body-parser": "^2.2.2",
    "bunyan": "^1.8.15",
    "bunyan-format": "^0.2.1",
    "compression": "^1.8.1",
    "connect-flash": "^0.1.1",
    "connect-redis": "^9.0.0",
    "contentful": "11.10.3",
    "cropperjs": "^2.1.0",
    "csrf-sync": "^4.2.1",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "express-session": "^1.19.0",
    "govuk-frontend": "5.14.0",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.1",
    "jquery": "3.7.1",
    "jwt-decode": "^4.0.0",
    "libphonenumber-js": "1.12.36",
    "multer": "^2.0.2",
    "nocache": "^4.0.0",
    "nunjucks": "^3.2.4",
    "opossum": "^9.0.0",
    "passport": "^0.7.0",
    "passport-oauth2": "^1.8.0",
    "redis": "^5.10.0",
    "superagent": "^10.3.0",
    "url-value-parser": "^2.2.0"
  },
  "devDependencies": {
    "@cypress/webpack-batteries-included-preprocessor": "^4.0.4",
    "@jgoz/esbuild-plugin-typecheck": "^4.0.4",
    "@ministryofjustice/eslint-config-hmpps": "^1.0.3",
    "@ministryofjustice/hmpps-precommit-hooks": "2.0.0",
    "@tsconfig/node24": "^24.0.4",
    "@types/bunyan": "^1.8.11",
    "@types/bunyan-format": "^0.2.9",
    "@types/compression": "^1.8.1",
    "@types/connect-flash": "0.0.40",
    "@types/cookie-session": "^2.0.49",
    "@types/express-session": "^1.18.2",
    "@types/http-errors": "^2.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "24.10.12",
    "@types/nunjucks": "^3.2.6",
    "@types/opossum": "^8.1.9",
    "@types/passport": "^1.0.17",
    "@types/passport-oauth2": "^1.8.0",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "^6.0.3",
    "audit-ci": "^7.1.0",
    "aws-sdk-client-mock": "^4.1.0",
    "cheerio": "^1.2.0",
    "cookie-session": "^2.1.1",
    "cypress": "^15.10.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^2.0.5",
    "del": "8.0.1",
    "esbuild": "^0.27.3",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-manifest": "^1.0.5",
    "esbuild-sass-plugin": "^3.6.0",
    "jest": "^30.2.0",
    "jest-html-reporter": "^4.3.0",
    "jest-junit": "^16.0.0",
    "jsonwebtoken": "^9.0.3",
    "lint-staged": "^16.2.7",
    "mocha-junit-reporter": "^2.2.1",
    "nock": "^14.0.10",
    "prettier": "^3.8.1",
    "prettier-plugin-jinja-template": "^2.1.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0"
  }
}
